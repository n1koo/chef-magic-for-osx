[alias]
  co = checkout
  ci = commit
  st = status
  pr = pull --rebase
  graph = log --graph --oneline --decorate
  standup = shortlog --since=yesterday

[user]
  email = <%= node["user"]["email"] %>
  name = <%= node["user"]["name"] %>

[push]
  default = current

[color]
  diff = auto
  status = auto
  branch = auto

[git-pair]
	pattern = dev+%name+%name@mynewsdesk.com
	authors = Jan Andersson <jan.andersson@mynewsdesk.com>
